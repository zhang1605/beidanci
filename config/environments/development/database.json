{
  "defaultConnection": "default",
  "connections": {
    "default": {
      "connector": "strapi-hook-mongoose",
      "settings": {
        "database": "beidanci",
        "host": "${process.env.DATABASE_HOST}",
        "srv": "true",
        "port": 27017,
        "username": "${process.env.DATABASE_USER}",
        "password": "${process.env.DATABASE_PASSWORD}"
      },
      "options": {
        "authenticationDatabase": "",
        "ssl": true
      }
    }
  }
}
